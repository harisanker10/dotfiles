        // "format": "Û∞çõ  {used} GB",
        "format": "Óâ¶ {used} GB",
        "max-length": 10
    },
    "tray": {
        "spacing": 10
    },
    "clock": {
        "tooltip-format": "{calendar}",
        "format-alt": "Ó™∞  {:%a, %d %b %Y}",
        "format": "Ôê∫  {:%I:%M %p}"
    },
    "network": {
